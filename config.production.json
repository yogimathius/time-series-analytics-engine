{
  "server": {
    "port": ":8080",
    "read_timeout": "30s",
    "write_timeout": "30s",
    "idle_timeout": "120s",
    "enable_pprof": false,
    "enable_metrics": true,
    "metrics_path": "/metrics",
    "health_path": "/health"
  },
  "storage": {
    "hot": {
      "max_series": 1000000,
      "max_points_per_series": 100000,
      "retention_period": "24h",
      "cleanup_interval": "1h",
      "memory_limit_mb": 4096,
      "enable_compression": true
    },
    "warm": {
      "enabled": true,
      "data_path": "/data/warm",
      "max_file_size_mb": 512,
      "retention_period": "720h",
      "compaction_interval": "6h",
      "compression_level": 6,
      "enable_encryption": true,
      "backup_enabled": true,
      "backup_interval": "24h"
    },
    "cold": {
      "enabled": true,
      "provider": "s3",
      "data_path": "/data/cold",
      "retention_period": "8760h",
      "compression_level": 9,
      "enable_encryption": true,
      "s3_bucket": "timeseries-cold-storage",
      "s3_region": "us-east-1",
      "lifecycle_management": true
    }
  },
  "ingestion": {
    "buffer_size": 10000,
    "batch_size": 1000,
    "flush_interval": "5s",
    "worker_pool_size": 16,
    "rate_limit_per_second": 100000,
    "enable_deduplication": true,
    "validation": {
      "max_value_range": 1000000000000,
      "allowed_metrics": [],
      "required_labels": ["tenant_id"],
      "max_label_length": 256,
      "max_metric_name_length": 512,
      "future_timestamp_threshold": "1h",
      "past_timestamp_threshold": "168h",
      "enable_schema_validation": true
    }
  },
  "analytics": {
    "anomaly_detection": {
      "enabled": true,
      "default_method": "zscore",
      "window_size": 100,
      "threshold": 3.0,
      "min_data_points": 10,
      "update_interval": "5m",
      "methods": ["zscore", "iqr", "isolation_forest"],
      "enable_seasonal_adjustment": true,
      "parallel_processing": true
    },
    "forecasting": {
      "enabled": true,
      "default_method": "holt_winters",
      "default_horizon": "24h",
      "update_interval": "1h",
      "methods": ["linear", "holt_winters", "arima"],
      "max_forecast_horizon": "168h",
      "confidence_intervals": true
    }
  },
  "enterprise": {
    "multitenancy": {
      "enabled": true,
      "default_plan": "standard",
      "resource_check_interval": "1m",
      "plans": {
        "starter": {
          "max_series": 1000,
          "max_points_per_day": 1000000,
          "max_query_rate": 10,
          "max_retention_days": 30,
          "max_storage_gb": 1,
          "max_concurrent_queries": 5
        },
        "standard": {
          "max_series": 10000,
          "max_points_per_day": 10000000,
          "max_query_rate": 100,
          "max_retention_days": 90,
          "max_storage_gb": 10,
          "max_concurrent_queries": 20
        },
        "enterprise": {
          "max_series": 100000,
          "max_points_per_day": 100000000,
          "max_query_rate": 1000,
          "max_retention_days": 365,
          "max_storage_gb": 100,
          "max_concurrent_queries": 100
        }
      }
    },
    "clustering": {
      "enabled": true,
      "node_id": "${NODE_ID}",
      "cluster_name": "production-cluster",
      "replication": {
        "factor": 3,
        "consistency_level": "eventual",
        "read_quorum": 2,
        "write_quorum": 2,
        "sync_timeout": "10s",
        "repair_interval": "6h",
        "enable_anti_entropy": true
      },
      "discovery": {
        "method": "static",
        "nodes": [
          "node1:8080",
          "node2:8080",
          "node3:8080"
        ]
      }
    },
    "security": {
      "auth_enabled": true,
      "jwt_secret": "${JWT_SECRET}",
      "jwt_expiration": "24h",
      "rate_limiting": {
        "enabled": true,
        "requests_per_second": 1000,
        "burst": 2000,
        "per_tenant": true
      },
      "encryption": {
        "enabled": true,
        "algorithm": "AES-256-GCM",
        "key_rotation_interval": "720h"
      },
      "audit_logging": {
        "enabled": true,
        "log_path": "/var/log/timeseries/audit.log",
        "include_data": false
      }
    }
  },
  "monitoring": {
    "prometheus": {
      "enabled": true,
      "port": ":9090",
      "path": "/metrics",
      "enable_histogram": true,
      "enable_summary": true
    },
    "logging": {
      "level": "info",
      "format": "json",
      "output": "stdout",
      "enable_caller": true,
      "enable_stacktrace": false
    },
    "profiling": {
      "enabled": false,
      "port": ":6060",
      "block_profile_rate": 0,
      "mutex_profile_fraction": 0
    },
    "distributed_tracing": {
      "enabled": true,
      "service_name": "time-series-analytics",
      "jaeger_endpoint": "http://jaeger:14268/api/traces",
      "sampling_rate": 0.1
    }
  },
  "performance": {
    "max_concurrent_queries": 1000,
    "query_timeout": "30s",
    "max_query_range": "168h",
    "enable_query_cache": true,
    "cache_size_mb": 1024,
    "cache_ttl": "1h",
    "enable_compression": true,
    "compression_algorithm": "snappy"
  },
  "backup": {
    "enabled": true,
    "schedule": "0 2 * * *",
    "retention_days": 30,
    "storage_backend": "s3",
    "encryption_enabled": true,
    "compression_enabled": true,
    "verify_backups": true
  }
}